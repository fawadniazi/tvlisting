<div class="espntv_desktop">
	<div class="espntv_heading">
		<p>TV Listing</p>
		<a href="http://www.foxsportsasia.com"><img src="img/logo_fox.png"></a>
	</div>

	<div id="espntv_main_promo">
		<div class="espntv_dummy"></div>
		<div class="espntv_promo">
			<img class="espntv_promo">
			<img class="espntv_buffer">
			<div class="espntv_msg"></div>
			<p class="espntv_msg"></p>
			<div class="espntv_video_container">
				<div class="dummy"></div>
				<div class="espntv_video"></div>
				<div class="espntv_video_controls">
					<img class="espntv_video_play" src="img/play_btn.png">
					<img class="espntv_video_pause" src="img/pause_btn.png">
					<div class="espntv_video_seek">
						<div class="espntv_video_play_bar"></div>
					</div>
				</div>
				<div class="espntv_video_start_container">
					<img class="espntv_video_start" src="img/video_play.png">
				</div>
			</div>
			<div class="espntv_slider">
				<ul></ul>
			</div>
			<img class="promo_controls prev" src="img/prev_btn.png">
			<img class="promo_controls pause" src="img/pause_btn.png">
			<img class="promo_controls play" src="img/play_btn.png">
			<img class="promo_controls next" src="img/next_btn.png">
		</div>
	</div>
	
	<div class="espntv_schedule">
		<div class="espntv_dummy_container">
			<div class="espntv_left_column">
				
				<div id="espntv_channels">
					<div class="espntv_showing">SHOWING NOW</div>
					<ul id="espntv_channel_list"></ul>
				</div>
			</div>
			<div class="espntv_right_column">
				<div id="espntv_select_date">
					<ul></ul>
				</div>
				<div id="espntv_time_line">
					<div class="time_line_container">
						<ul id="espntv_time_list"></ul>
					</div>
					<div class="espntv_time_arrow_left"><span class="dummy"></span><img src="img/time_arrow_left.png"></div>
					<div class="espntv_time_arrow_right"><span class="dummy"></span><img src="img/time_arrow_right.png"></div>
				</div>
				<div id="espntv_programs">
					<div class="programs_schedule"></div>
					<div class="load_prev hour"><div class="load_bg"></div><img src="img/icon_PrevHr.png"></div>
					<div class="load_next hour"><div class="load_bg"></div><img src="img/icon_NextHr.png"></div>
					<div class="load_prev day"><div class="load_bg"></div><img src="img/icon_PrevDay.png"></div>
					<div class="load_next day"><div class="load_bg"></div><img src="img/icon_NextDay.png"></div>
				</div>
			</div>
		</div>
		<img class="espntv_bottom_live_icon" src="img/live_icon.png">
		<div id="espntv_program_details">
			<div class="espntv_details_bg"></div>
			<div class="espntv_details">
				<img class="espntv_details_channel">
				<div class="espntv_details_content">
					<p class="espntv_sub_genre">FOOTBALL</p>
					<p class="espntv_date">WED 3 Oct</p>
					<p class="espntv_time">3:00 PM - 7:00 PM</p>
					<p class="espntv_programme">Baltimore Orioles vs. Tampa Bay Rays</p>
					<p class="espntv_matchup">MLB Regular Season 2012</p>
					<p class="espntv_replay">Replay</p>
					<p class="espntv_replay_date">4 Oct</p><span>03:30 AM</span><br/>
					<p class="espntv_replay_date">9 Oct</p><span>03:30 AM</span><br/>
					<p class="espntv_replay_date">10 Oct</p><span>03:30 AM</span><br/>
					<p class="espntv_channel_name"></p>
					<img class="espntv_details_live" src="img/live_indicator.png">
				</div>
				<div class="espntv_details_share">
					<p>SHARE</p>
					<a class="espntv_details_social twitter"><img src="img/twitter.png" alt="Share on Twitter"></a>
					<a class="espntv_details_social facebook"><img src="img/facebook.png" alt="Share on Facebook"></a>
					<a class="espntv_details_social gplus"><img src="img/gplus.png" alt="Share on Google +"></a>
				</div>
				<a class="espntv_details_close"><img src="img/details_close.png"></a>
			</div>
		</div>
		<div id="espntv_select_region">
			<p>Select Region</p>
			<div class="espntv_selected_region">
				<img src="img/down_arrow.png">
				<p>China</p>
			</div>
			<ul id="espntv_region_list"></ul>
		</div>
	</div>
	<div class="espntv_bottom">
		<!-- <img src="img/bottom_img.png"> -->
	</div>
</div>